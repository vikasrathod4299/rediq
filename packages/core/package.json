{
    "name": "flexq",
    "version": "0.1.0",
    "description": "Lightweight job queue with pluggable storage - memory or Redis, Postgres. Same  API, swap backends.",
    "author": "Vikas Rathod",
    "license": "MIT",
    "keywords": [
        "job-queue",
        "task-queue",
        "async-queue",
        "backpressure",
        "redis",
        "postgres",
        "worker",
        "in-memory-queue",
        "lightweight"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports" : {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],

    "engines" : {
        "node": ">=16.0.0"
    },
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run build"
    }
}