{
    "name": "@flexq/redis",
    "version": "0.1.0",
    "description": "Redis storage adapter for flexq job queue",
    "author": "Vikas Rathod",
    "license": "MIT",
    "keywords": [
        "flexq",
        "redis",
        "job-queue",
        "async-queue",
        "storage-adapter"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js"
        }
    },

    "files": [
        "dist" 
    ],
    "engines": {
        "node": ">=16.0.0"
    },

    "scripts": {
        "build": "tsc && cp -r src/lua-scripts dist/lua-scripts",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run build"
    },

    "dependencies": {
        "flexq": "^0.1.0"
    },
    "peerDependencies": {
        "ioredis": "^5.0.0"
    },
    "devDependencies": {
        "ioredis": "^5.9.2",
        "@types/ioredis": "^4.28.10"
    }
}