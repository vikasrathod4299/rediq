{
  "name": "flexmq-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:core": "npm run build -w packages/core",
    "build:redis": "npm run build -w packages/redis",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "npm run clean --workspaces"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^25.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "minimatch": "^10.2.1"
  }
}
